{
  "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "**/*.js.map": true,
      "**/*.js": {"when": "$(basename).ts"}
  },
  "search.exclude": {
      "platforms": true,
      "node_modules": true,
      "hooks": true,
      "**/*.js": true,
      "**/*.js.map": true
  },
  "autoimport.filesToScan": "./app/**/*.{ts,tsx}",
}
